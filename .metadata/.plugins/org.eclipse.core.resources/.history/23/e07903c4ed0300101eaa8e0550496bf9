package builder_7;

public class Main {
	public static void main(String[] args) {
		 // Usando el Builder para construir un Producto
        Producto producto = new ProductoBuilder()
                                .nombre("Camiseta")
                                .precio(20.0)
                                .build();


        // Imprimir detalles del producto
        System.out.println(producto);
	}
}
