package singleton14perros;

public class PerroSingleton {
	private static PerroSingleton instance;
	private String raza;
	private int edad;
	
	private PerroSingleton() {}
	
	public static PerroSingleton getInstance() {
		if(instance==null) {
			instance = new PerroSingleton()
		}
		return instance;
	}
}
